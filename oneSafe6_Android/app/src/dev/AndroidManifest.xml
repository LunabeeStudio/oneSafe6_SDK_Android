<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <queries>
        <package android:name="com.lunabee.onesafe.dev" />
    </queries>

    <application>
        <profileable
            android:shell="true"
            tools:targetApi="q" />
        <service
            android:name=".messagecompanion.OSAccessibilityService"
            android:exported="true"
            android:label="@string/oneSafeK_accessibilityService_label"
            android:permission="android.permission.BIND_ACCESSIBILITY_SERVICE">
            <intent-filter>
                <action android:name="android.accessibilityservice.AccessibilityService" />
            </intent-filter>
            <meta-data
                android:name="android.accessibilityservice"
                android:resource="@xml/os_accessibility_sercvice_config" />
        </service>

    </application>
</manifest>